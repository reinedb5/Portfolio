   "source": [
    "def get_bus_indexes(df)->list:\n",
    "    bus_mean = df['bus'].mean()\n",
    "    bus_indexes = df[df['bus'] > 2 * bus_mean].index.tolist()\n",
    "    return bus_indexes"
   ]
  },
